{
	"name": "project-name",
	"version": "1.0.0",
	"description": "celtics.com",
	"author": "Kyle George",
	"license": "MIT",
	"scripts": {
		"watch": "parcel ./src/index.html --out-dir dist/js --port 3434 & onchange 'src/html/*' 'src/html' -- touch ./src/index.html",
		"stage": "git commit -a -m 'update' && git push 'https://github.com/kylepg/kylepg.github.io.git' master -f",
		"publish": "git commit -a -m 'update' && git push 'https://github.com/v1v2v3co/v1v2v3co.git' master"
	},
	"eslintIgnore": [
		"index.js",
		"dist/**/*.js",
		"dist/*.js",
		"node_modules/"
	],
	"eslintConfig": {
		"extends": [
			"airbnb",
			"prettier"
		],
		"rules": {
			"no-console": 0,
			"quotes": [
				"error",
				"backtick"
			],
			"func-names": 0,
			"no-constant-condition": 0,
			"wrap-iife": 0,
			"spaced-comment": 0,
			"space-before-function-paren": "off",
			"no-var": "off",
			"no-plusplus": "off",
			"prefer-destructuring": "off",
			"no-global-assign": "off",
			"import/extensions": 0,
			"global-require": "off",
			"ignoreStrings": true,
			"prettier/prettier": [
				"error",
				{
					"trailingComma": "es5",
					"singleQuote": true,
					"printWidth": 200
				}
			]
		},
		"env": {
			"browser": true,
			"node": true,
			"jquery": true
		},
		"parser": "babel-eslint",
		"plugins": [
			"prettier"
		]
	},
	"babel": {
		"sourceMap": "inline",
		"presets": [
			[
				"env",
				{
					"targets": {
						"node": "current"
					}
				}
			]
		],
		"plugins": [
			"transform-runtime"
		],
		"minified": true
	},
	"devDependencies": {
		"autoprefixer": "^9.0.2",
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.2.6",
		"babel-plugin-transform-async-to-generator": "^6.24.1",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-es2015": "^6.24.1",
		"babelify": "^8.0.0",
		"eslint": "^4.15.0",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-config-prettier": "^2.9.0",
		"eslint-plugin-import": "^2.13.0",
		"eslint-plugin-jsx-a11y": "^6.1.1",
		"eslint-plugin-prettier": "^2.6.2",
		"eslint-plugin-react": "^7.10.0",
		"grunt": "^1.0.4",
		"grunt-browserify": "^5.3.0",
		"grunt-cli": "^1.3.2",
		"grunt-contrib-sass": "^1.0.0",
		"grunt-contrib-watch": "^1.0.0",
		"grunt-notify": "^0.4.5",
		"node-sass": "^7.0.0",
		"nodemon": "^1.18.9",
		"onchange": "^5.2.0",
		"prettier-eslint": "^8.8.2",
		"uglifyify": "^4.0.5"
	},
	"dependencies": {
		"babel-polyfill": "^6.26.0",
		"babel-runtime": "^6.26.0",
		"jquery": "^3.3.1",
		"js-cookie": "^2.2.0",
		"npm": "^6.9.0"
	}
}
